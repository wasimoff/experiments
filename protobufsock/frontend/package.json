  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.0.0",
    "@bufbuild/protoc-gen-es": "^2.0.0",
    "@msgpack/msgpack": "^3.0.0-beta2",
    "it-length-prefixed": "^9.0.5",
    "it-reader": "^6.0.4",
    "protobufjs": "^7.3.2",
    "protobufjs-cli": "^1.1.2",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.5",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "npm-run-all2": "^6.1.2",
    "typescript": "~5.4.0",
    "vite": "^5.2.8",
    "vue-tsc": "^2.0.11"
  }
